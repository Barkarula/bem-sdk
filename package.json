{
  "name": "@bem/deps",
  "version": "0.1.0",
  "description": "Manage BEM dependencies",
  "license": "MPL-2.0",
  "author": "Andrew Abramov <mail@blond.im>",
  "keywords": [
    "bem",
    "deps",
    "dependencies",
    "deps.js",
    "read",
    "parse",
    "resolve"
  ],
  "repository": "bem-sdk/bem-deps",
  "engines": {
    "node": ">= 4"
  },
  "main": "lib/index.js",
  "files": [
    "lib/**"
  ],
  "dependencies": {
    "bem-naming": "1.0.1",
    "bem-walk": "1.0.0-1",
    "lodash": "4.12.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "istanbul": "^0.4.3",
    "jscs": "^3.0.3",
    "jshint": "^2.9.2",
    "matcha": "^0.7.0",
    "mocha": "^2.4.5",
    "stream-to-array": "^2.3.0",
    "through2": "^2.0.1"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "npm run spec && npm run unit-test",
    "lint": "jshint . && jscs . -c .jscs.js",
    "spec": "mocha -R spec --opts spec/mocha.opts spec/",
    "unit-test": "mocha -R spec --opts test/mocha.opts test/",
    "cover": "istanbul cover _mocha spec/ test/",
    "coveralls": "npm i coveralls && npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  }
}
