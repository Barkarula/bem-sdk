  "repository": "git://github.com/andrewblond/bem-decl",
  "homepage": "https://github.com/andrewblond/bem-decl",
  "bugs": "https://github.com/andrewblond/bem-decl/issues",
  "contributors": "Andrew Abramov <andrewblond@yandex.com>",
  "maintainers": [
    {
      "name": "Andrew Abramov",
