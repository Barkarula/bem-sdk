{
  "name": "bem-deps",
  "version": "0.0.10",
  "description": "Manage BEM dependencies",
  "license": "MPL-2.0",
  "author": "Andrew Abramov <mail@blond.im>",
  "keywords": [
    "bem",
    "deps",
    "dependencies",
    "deps.js",
    "read",
    "parse",
    "resolve"
  ],
  "repository": "bem-sdk/bem-deps",
  "engines": {
    "node": ">= 0.10.0"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "LICENSE.txt",
    "README.txt"
  ],
  "dependencies": {
    "babel-polyfill": "6.3.14",
    "bem-naming": "0.5.1",
    "bem-walk": "1.0.0-alpha1",
    "lodash": "3.10.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "6.3.13",
    "babel-register": "6.3.13",
    "chai": "3.4.1",
    "gulp": "3.9.0",
    "gulp-babel": "6.1.1",
    "istanbul": "0.3.14",
    "jscs": "1.13.1",
    "jshint": "2.8.0",
    "lodash": "3.9.3",
    "matcha": "0.6.0",
    "mocha": "2.2.5",
    "stream-to-array": "2.2.0",
    "through2": "2.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "prepublish": "npm run clean && npm run dist",
    "pretest": "npm run lint",
    "test": "npm run spec && npm run unit-test",
    "lint": "jshint . && jscs . -c .jscs.js",
    "spec": "mocha -R spec --opts spec/mocha.opts spec/",
    "unit-test": "mocha -R spec --opts test/mocha.opts test/",
    "cover": "istanbul cover _mocha --opts spec/mocha.opts",
    "coveralls": "npm i coveralls && npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "dist": "gulp"
  }
}
