{
  "name": "@bem/sdk.deps",
  "version": "0.0.1",
  "description": "Manage BEM dependencies",
  "publishConfig": {
    "access": "public"
  },
  "license": "MPL-2.0",
  "author": "Andrew Abramov <mail@blond.im>",
  "keywords": [
    "bem",
    "deps",
    "dependencies",
    "deps.js",
    "read",
    "parse",
    "resolve"
  ],
  "repository": "bem/bem-sdk",
  "bugs": {
    "url": "https://github.com/bem/bem-sdk/issues?q=tag%3Dpkg%3Adeps"
  },
  "homepage": "https://github.com/bem/bem-sdk/tree/master/packages/deps#readme",
  "engines": {
    "node": ">= 4.6.2"
  },
  "main": "lib/index.js",
  "files": [
    "lib/**"
  ],
  "dependencies": {
    "@bem/sdk.decl": "*",
    "@bem/sdk.entity-name": "*",
    "@bem/sdk.graph": "*",
    "@bem/sdk.walk": "*",
    "debug": "^2.2.0",
    "mz": "^2.4.0",
    "node-eval": "1.1.0"
  },
  "devDependencies": {
    "ava": "^0.19.0",
    "coveralls": "^2.11.9",
    "stream-to-array": "^2.3.0",
    "through2": "^2.0.1"
  },
  "scripts": {
    "test": "nyc ava",
    "unit-test": "ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "failWithoutAssertions": false,
    "source": [
      "lib/**/*.js"
    ],
    "concurrency": 25
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
