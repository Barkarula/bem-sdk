{
  "name": "@bem/sdk.walk",
  "version": "0.2.0",
  "description": "Walk easily thru BEM file structure",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "bem",
    "walk",
    "level",
    "scheme",
    "nested",
    "flat"
  ],
  "author": "Andrew Abramov <mail@blond.im> (github.com/blond)",
  "license": "MPL-2.0",
  "repository": "bem/bem-sdk",
  "engines": {
    "node": ">= 4.0"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "dependencies": {
    "@bem/sdk.cell": "^0.2.0",
    "@bem/sdk.entity-name": "^0.2.0",
    "@bem/sdk.file": "^0.2.0",
    "@bem/sdk.naming.entity": "^0.2.0",
    "async-each": "1.0.1"
  },
  "devDependencies": {
    "ava": "^0.19.0",
    "benchmark": "^2.1.0",
    "coveralls": "^2.11.9",
    "promise-map-series": "^0.2.2",
    "stream-to-array": "^2.3.0"
  },
  "scripts": {
    "test": "nyc ava",
    "unit-test": "ava",
    "bench": "npm run bench-deps && node ./bench/run.js",
    "bench-deps": "cd bench && npm i && cd fixtures && bower i",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "ava": {
    "serial": true,
    "verbose": true,
    "files": [
      "test/**/*.test.js"
    ]
  }
}
